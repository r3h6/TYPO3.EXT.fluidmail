{
    "name": "r3h6/fluidmail",
    "type": "typo3-cms-extension",
    "description": "Use fluid for creating e-mail messages.",
    "keywords": [
        "TYPO3",
        "extension",
        "fluidmail"
    ],
    "authors": [
        {
            "name": "R3 H6",
            "email": "r3h6@outlook.com",
            "role": "Developer"
        }
    ],
    "license": "GPL-3.0+",
    "require": {
        "typo3/cms-core": "~6.2.14|>=7.4.0,<8.0",
        "html2text/html2text": "~3.0.0"
    },
    "require-dev": {
        "mikey179/vfsStream": "1.4.*@dev",
        "phpunit/phpunit": "~4.7.0"
    },
    "autoload": {
        "psr-4": {
            "TYPO3\\Fluidmail\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TYPO3\\Fluidmail\\Tests\\": "Tests",
            "TYPO3\\CMS\\Core\\Tests\\": ".Build/vendor/typo3/cms/typo3/sysext/core/Tests/"
        }
    },
    "replace": {
        "fluidmail": "self.version",
        "typo3-ter/fluidmail": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    }
}